    -- æ–¹æ³•3: æ–­å¼€ç½‘ç»œè¿æ¥
    local success3 = pcall(function()
        game:GetService("NetworkClient"):Disconnect()
        return true
    end)
    
    if success3 then
        print("âœ… ä½¿ç”¨æ–­å¼€è¿æ¥æ–¹æ³•æˆåŠŸ")
        return
    end
    
    -- æ–¹æ³•4: åˆ¶é€ é”™è¯¯å¯¼è‡´æ–­å¼€
    local success4 = pcall(function()
        error("å¼ºåˆ¶æ–­å¼€è¿æ¥")
    end)
    
    -- æ–¹æ³•5: ç ´åæœ¬åœ°ç©å®¶è„šæœ¬
    local success5 = pcall(function()
        if localPlayer:FindFirstChild("PlayerScripts") then
            localPlayer.PlayerScripts:Destroy()
        end
        return true
    end)
    
    if success5 then
        print("âœ… ä½¿ç”¨è„šæœ¬ç ´åæ–¹æ³•æˆåŠŸ")
